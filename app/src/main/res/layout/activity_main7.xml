<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity7">

    <TextView
        android:id="@+id/textView12"
        android:layout_width="238dp"
        android:layout_height="38dp"
        android:layout_marginStart="80dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="523dp"
        android:fontFamily="@font/lato_bold"
        android:text="@string/map_sick"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="24dp"
        app:layout_constraintBottom_toTopOf="@+id/bottom_panel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="120dp"
        android:layout_marginEnd="120dp"
        android:layout_marginBottom="397dp"
        app:layout_constraintBottom_toTopOf="@+id/bottom_panel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/sick" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="371dp"
        android:layout_height="26dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="356dp"
        android:fontFamily="@font/lato"
        android:text="@string/title_sick"
        android:textColor="@color/grey"
        app:layout_constraintBottom_toTopOf="@+id/bottom_panel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView20" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="370dp"
        android:layout_height="43dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="312dp"
        android:fontFamily="@font/lato"
        android:text="@string/description_sick"
        android:textColor="@color/grey"
        app:layout_constraintBottom_toTopOf="@+id/bottom_panel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <EditText
        android:id="@+id/name"
        android:layout_width="366dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:width="335dp"
        android:height="60dp"
        android:backgroundTint="@color/input_email"
        android:background="#B8C1CC"
        android:ems="10"
        android:textSize="15dp"
        android:fontFamily="@font/lato"
        android:text="  Эдуард"
        android:hint="    Имя"
        android:inputType="text"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/ot"
        android:layout_width="366dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="20dp"
        android:width="335dp"
        android:height="60dp"
        android:background="@color/input_email"

        android:backgroundTint="@color/input_email"
        android:ems="10"
        android:fontFamily="@font/lato"
        android:text="  Олегович"
        android:hint="    Отчество"
        android:inputType="text"
        android:textSize="15dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/surname"
        android:layout_width="366dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="20dp"
        android:width="335dp"
        android:height="60dp"
        android:text=" Тицкий"
        android:background="@color/input_email"
        android:backgroundTint="@color/input_email"
        android:ems="10"
        android:fontFamily="@font/lato"
        android:hint="    Фамилия"
        android:inputType="text"
        android:textSize="15dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ot"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/birthday"
        android:layout_width="366dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="20dp"
        android:width="335dp"
        android:text=" 28 февраля 1991"
        android:height="60dp"
        android:background="@color/input_email"
        android:backgroundTint="@color/input_email"
        android:ems="10"
        android:textSize="15dp"
        android:fontFamily="@font/lato"
        android:hint="    Дата рождения"
        android:inputType="text"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/surname"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/pol"
        android:layout_width="366dp"
        android:layout_height="40dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="20dp"
        android:text=" Мужской"
        android:width="335dp"
        android:height="60dp"
        android:background="@color/input_email"
        android:backgroundTint="@color/input_email"
        android:ems="10"
        android:textSize="15dp"
        android:fontFamily="@font/lato"
        android:hint="    Пол"
        android:inputType="text"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/birthday"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/save"
        android:layout_width="335dp"
        android:layout_height="48dp"
        android:layout_weight="1"
        android:height="24dp"
        android:backgroundTint="@color/blue_skip"
        android:onClick="save_information"
        android:text="@string/save"
        android:textColor="@color/white"
        android:textSize="15dp"
        app:cornerRadius="10dp"
        app:layout_constraintBottom_toTopOf="@+id/bottom_panel"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pol" />

    <LinearLayout
        android:id="@+id/bottom_panel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="600dp"
        android:background="@color/white"
        android:backgroundTint="@color/white"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="61dp"
            android:layout_height="55dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:onClick="goToAnalisy"
            app:srcCompat="@drawable/tabbar2_3" />

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:onClick="goToEmptyActivity"
            app:srcCompat="@drawable/tabbar2" />

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:layout_weight="1"
            android:onClick="goToEmptyActivity"
            app:srcCompat="@drawable/tabbar3" />

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="90dp"
            android:layout_height="match_parent"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/tabbar2_2" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>